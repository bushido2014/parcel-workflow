  "version": "1.0.0",
  "description": "Simple Workflow with Parcel and Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache",
    "dev": "parcel ./src/index.html --open --no-autoinstall",
    "prod": "parcel build ./src/index.html --no-source-maps --public-url ./"
