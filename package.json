    "url": "https://github.com/bushido2014/parcel-workflow/issues"
  },
  "homepage": "https://github.com/bushido2014/parcel-workflow#readme",
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "parcel": "^1.12.4",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  }
}
